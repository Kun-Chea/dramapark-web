{
  "name": "ant-design-pro",
  "version": "6.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "build": "max build",
    "start": "npm run dev",
    "start:local": "cross-env PORT=8000 UMI_ENV=localhome max dev",
    "start:dev": "cross-env PORT=8000 UMI_ENV=develop max dev",
    "start:prod": "cross-env PORT=8000 UMI_ENV=production max dev",
    "build:dev": "cross-env UMI_ENV=develop max build",
    "build:prod": "cross-env UMI_ENV=production max build",
    "format": "prettier --cache --write .",
    "postinstall": "max setup",
    "prepare": "husky",
    "setup": "max setup",
    "build:version": "react-scripts build && echo \"export const VERSION = '%npm_package_version%';\" > public/version.js"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^5.5.0",
    "@ant-design/plots": "^2.3.2",
    "@ant-design/pro-components": "^2.7.19",
    "@ant-design/use-emotion-css": "1.0.4",
    "@testing-library/dom": "^10.4.0",
    "@umijs/route-utils": "^4.0.1",
    "antd": "^5.21.1",
    "antd-style": "^3.6.2",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "fabric": "^6.4.0",
    "highlight.js": "^11.10.0",
    "html-to-image": "^1.11.13",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "omit.js": "^2.0.2",
    "query-string": "^9.1.0",
    "rc-menu": "^9.15.0",
    "rc-util": "^5.43.0",
    "react": "^18.3.0",
    "react-cropper": "^2.3.3",
    "react-dev-inspector": "^2.0.1",
    "react-dom": "^18.3.0",
    "react-helmet-async": "^2.0.0",
    "react-highlight": "^0.15.0",
    "xgplayer": "^3.0.22",
    "xgplayer-hls": "^3.0.22",
    "xgplayer-subtitles": "^3.0.22"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^3.3.0",
    "@testing-library/react": "^16.0.1",
    "@types/classnames": "^2.3.1",
    "@types/express": "^4.17.21",
    "@types/history": "^4.7.11",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.4",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "@umijs/fabric": "^2.14.1",
    "@umijs/lint": "^4.2.9",
    "@umijs/max": "^4.2.9",
    "cross-env": "^7.0.3",
    "eslint": "^9.11.0",
    "express": "^4.21.0",
    "gh-pages": "^6.1.0",
    "husky": "^9.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "mockjs": "^1.1.0",
    "prettier": "^3.3.0",
    "swagger-ui-dist": "^5.17.14",
    "tailwindcss": "^3",
    "ts-node": "^10.9.1",
    "typescript": "^5.6.2",
    "umi-presets-pro": "^2.0.0"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
